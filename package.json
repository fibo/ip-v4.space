{
  "name": "ip-v4.space",
  "version": "0.0.0",
  "private": true,
  "description": "is an IPv4 space visualization, your eye to see the Internet as we imagined in the 90s",
  "main": "src/index.js",
  "scripts": {
    "browserify": "NODE_ENV=production browserify $npm_package_main -t envify -t babelify | uglifyjs --compress > bundle.js",
    "build": "npm run browserify; npm run sass",
    "check-deps": "npm outdated",
    "predeploy_css": "npm run sass",
    "deploy": "for x in css js html media; do npm run deploy_${x}; done",
    "deploy_css": "aws s3 cp style.css s3://ip-v4.space/style.css",
    "predeploy_js": "npm run browserify",
    "deploy_js": "aws s3 cp bundle.js s3://ip-v4.space/bundle.js",
    "deploy_html": "for x in index 404; do npm run deploy_${x}_html; done",
    "deploy_index_html": "aws s3 cp index.html s3://ip-v4.space/index.html",
    "deploy_404_html": "aws s3 cp 404.html s3://ip-v4.space/404.html",
    "deploy_media": "aws s3 sync media s3://ip-v4.space/media",
    "lint": "npm run lint_css; npm run lint_js",
    "lint_css": "sass-lint -v src/sass/*",
    "lint_js": "cd src; standard; cd ..",
    "sass": "node-sass src/sass/style.scss --output-style compressed --output .",
    "sass_watch": "node-sass src/sass/style.scss --watch --output .",
    "start": "budo -l -o -s bundle.js $npm_package_main",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint",
    "check-deps"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/ip-v4.space.git"
  },
  "keywords": [
    "ip",
    "ipv4",
    "visual"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/ip-v4.space/issues"
  },
  "homepage": "http://ip-v4.space",
  "devDependencies": {
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "budo": "^10.0.3",
    "envify": "^4.0.0",
    "node-sass": "^4.5.2",
    "pre-commit": "^1.2.2",
    "sass-lint": "^1.10.2",
    "uglify-js": "^2.8.22"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "static-props": "^1.0.2"
  }
}
